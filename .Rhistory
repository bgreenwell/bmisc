library(misc)
clip <- function(x, interval, ...) {
UseMethod("clip")
}
#' @export
clip.numeric <- function(x, ...) {
x[x < interval[1L]] <- interval[1L]
x[x > interval[2L]] <- interval[2L]
x
}
clip(rnorm(10), interval = c(0, 1))
#' @export
