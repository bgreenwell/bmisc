#' @export
clip.matrix <- function(x, interval, ...) {
x[x < interval[1L]] <- interval[1L]
x[x > interval[2L]] <- interval[2L]
x
}
clip(matrix(rnorm(25), 5, 5), interval = c(0, 1))
