## The function is currently defined as
function (object, newdata, important = FALSE) 
{
    data <- if (missing(newdata)) 
        eval(object$call$data)
    else newdata
    if (important) {
        data <- data[, c(names(object$variable.importance), all.vars(formula(object)[[2]]))]
    }
    data$node <- as.factor(assignNode(object, newdata = data))
    data
  }
}
